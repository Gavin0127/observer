{
  "$ref": "#/definitions/Track",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Track": {
      "additionalProperties": false,
      "properties": {
        "audioLevel": {
          "type": "number"
        },
        "concealedSamples": {
          "type": "number"
        },
        "concealmentEvents": {
          "type": "number"
        },
        "detached": {
          "type": "boolean"
        },
        "ended": {
          "type": "boolean"
        },
        "frameHeight": {
          "type": "number"
        },
        "frameWidth": {
          "type": "number"
        },
        "framesDecoded": {
          "type": "number"
        },
        "framesDropped": {
          "type": "number"
        },
        "framesReceived": {
          "type": "number"
        },
        "framesSent": {
          "type": "number"
        },
        "hugeFramesSent": {
          "type": "number"
        },
        "id": {
          "type": "string"
        },
        "insertedSamplesForDeceleration": {
          "type": "number"
        },
        "jitterBufferDelay": {
          "type": "number"
        },
        "jitterBufferEmittedCount": {
          "type": "number"
        },
        "kind": {
          "enum": [
            "audio",
            "video"
