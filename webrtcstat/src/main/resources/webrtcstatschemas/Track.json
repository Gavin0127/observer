          "type": "string"
        },
        "mediaSourceId": {
          "type": "string"
        },
        "remoteSource": {
          "type": "boolean"
        },
        "removedSamplesForAcceleration": {
          "type": "number"
        },
        "silentConcealedSamples": {
          "type": "number"
        },
        "timestamp": {
          "type": "string"
        },
        "totalAudioEnergy": {
          "type": "number"
        },
        "totalSamplesDuration": {
          "type": "number"
        },
        "totalSamplesReceived": {
          "type": "number"
        },
        "trackIdentifier": {
          "type": "string"
        },
        "type": {
          "enum": [
            "track"
          ],
          "type": "string"
        }
      },
      "required": [
        "detached",
        "ended",
        "id",
        "kind",
        "mediaSourceId",
        "remoteSource",
        "timestamp",
        "trackIdentifier",
        "frameHeight",
        "frameWidth",
        "framesSent",
        "hugeFramesSent",
        "audioLevel",
        "concealedSamples",
        "concealmentEvents",
        "insertedSamplesForDeceleration",
        "jitterBufferDelay",
        "jitterBufferEmittedCount",
        "removedSamplesForAcceleration",
        "silentConcealedSamples",
        "totalAudioEnergy",
        "totalSamplesDuration",
        "totalSamplesReceived",
        "framesDecoded",
        "framesDropped",
        "framesReceived",
        "type"
