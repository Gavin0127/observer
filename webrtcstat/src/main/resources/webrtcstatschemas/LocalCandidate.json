{
  "$ref": "#/definitions/LocalCandidate",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "LocalCandidate": {
      "additionalProperties": false,
      "properties": {
        "candidateType": {
          "enum": [
            "host",
            "srflx",
            "prflx",
            "relay"
          ],
          "type": "string"
        },
        "deleted": {
          "type": "boolean"
        },
        "id": {
          "type": "string"
        },
        "ip": {
          "type": "string"
        },
        "isRemote": {
          "enum": [
            false
          ],
          "type": "boolean"
        },
        "networkType": {
          "enum": [
            "bluetooth",
            "cellular",
            "ethernet",
            "wifi",
            "wimax",
            "vpn",
            "unknown"
          ],
          "type": "string"
        },
        "port": {
          "type": "number"
        },
        "priority": {
          "type": "number"
        },
        "protocol": {
          "enum": [
            "udp",
            "tcp"
          ],
          "type": "string"
        },
        "timestamp": {
          "type": "number"
        },
        "transportId": {
          "type": "string"
        },
        "type": {
          "enum": [
            "local-candidate"
          ],
          "type": "string"
        }
      },
      "required": [
        "candidateType",
        "deleted",
        "id",
        "ip",
        "isRemote",
        "networkType",
        "port",
        "priority",
        "protocol",
        "timestamp",
        "transportId",
        "type"
      ],
      "type": "object"
    }
  }
}