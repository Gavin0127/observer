{
  "$ref": "#/definitions/CandidatePair",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "CandidatePair": {
      "additionalProperties": false,
      "properties": {
        "availableOutgoingBitrate": {
          "type": "number"
        },
        "bytesReceived": {
          "type": "number"
        },
        "bytesSent": {
          "type": "number"
        },
        "consentRequestsSent": {
          "type": "number"
        },
        "currentRoundTripTime": {
          "type": "number"
        },
        "id": {
          "type": "string"
        },
        "localCandidateId": {
          "type": "string"
        },
        "nominated": {
          "type": "boolean"
        },
        "priority": {
          "type": "number"
        },
        "remoteCandidateId": {
          "type": "string"
        },
        "requestsReceived": {
          "type": "number"
        },
        "requestsSent": {
          "type": "number"
        },
        "responsesReceived": {
          "type": "number"
        },
        "responsesSent": {
          "type": "number"
        },
        "state": {
          "enum": [
            "frozen",
            "waiting",
            "in-progress",
            "failed",
            "succeeded"
          ],
          "type": "string"
        },
        "timestamp": {
          "type": "number"
        },
        "totalRoundTripTime": {
          "type": "number"
        },
        "transportId": {
          "type": "string"
        },
        "type": {
          "enum": [
            "candidate-pair"
          ],
          "type": "string"
        },
        "writable": {
          "type": "boolean"
        }
      },
      "required": [
        "availableOutgoingBitrate",
        "bytesReceived",
        "bytesSent",
        "consentRequestsSent",
        "currentRoundTripTime",
        "id",
        "localCandidateId",
        "nominated",
        "priority",
        "remoteCandidateId",
        "requestsReceived",
        "requestsSent",
        "responsesReceived",
        "responsesSent",
        "state",
        "timestamp",
        "totalRoundTripTime",
        "transportId",
        "type",
        "writable"
      ],
      "type": "object"
    }
  }
}